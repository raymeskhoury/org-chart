<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<OfficeApp xmlns="http://schemas.microsoft.com/office/appforoffice/1.1"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xmlns:bt="http://schemas.microsoft.com/office/officeappbasictypes/1.0" xsi:type="ContentApp">
  <Id>e644da98-66d8-4416-aec7-8ca75294ff8c</Id>
  <Version>1.0.0.0</Version>
  <ProviderName>Ray Khoury Software</ProviderName>
  <DefaultLocale>en-US</DefaultLocale>
  <DisplayName DefaultValue="Org Chart Generator"/>
  <Description DefaultValue="An add-in which will generate an org chart based on data you provide. Charts can be exported in PNG, SVG or PDF formats."/>
  <IconUrl DefaultValue="https://raymeskhoury.github.io/org-chart/assets/icon-32.png"/>
  <HighResolutionIconUrl DefaultValue="https://raymeskhoury.github.io/org-chart/assets/icon-64.png"/>
  <AppDomains>
    <AppDomain>https://localhost:3000</AppDomain>
  </AppDomains>
  <Hosts>
    <Host Name="Workbook"/>
  </Hosts>
  <DefaultSettings>
    <SourceLocation DefaultValue="https://raymeskhoury.github.io/org-chart/index.html"/>
    <RequestedWidth>700</RequestedWidth>
    <RequestedHeight>400</RequestedHeight>
  </DefaultSettings>
  <Permissions>ReadWriteDocument</Permissions>
</OfficeApp>